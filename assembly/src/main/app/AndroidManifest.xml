<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.assembly">

    <application
        android:name="com.app.ModuleApp"
        android:allowBackup="true"
        android:label="assembly"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/aroute_AppTheme">

        <activity android:name=".TestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.assembly.ARouterActivity" />
        <activity android:name="com.assembly.InterceptActivity" />
        <activity android:name="com.assembly.ErrorActivity" />
        <activity android:name="com.assembly.BeInterceptActivity" />
    </application>
</manifest>
